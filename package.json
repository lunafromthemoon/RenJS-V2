  },
  "homepage": "https://github.com/lunafromthemoon/RenJS#readme",
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^3.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.7.0",
